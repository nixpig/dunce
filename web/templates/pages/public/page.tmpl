{{ $params := .Context.Queries }}
{{ $articles := (call .Api.GetArticlesByTypeName) }}

{{ if $articles }}
	<ul>
		{{ range $article := $articles }}
			<li>
				<a href="/{{ $article.TypeName }}/{{ $article.Slug }}">
					{{ $article.Title }}
				</a> on {{ $article.UpdatedAt }}
			</li>
		{{ end }}
	</ul>

{{ else }}
	<div>No articles yet. You best go write some!</div>
{{ end }}
